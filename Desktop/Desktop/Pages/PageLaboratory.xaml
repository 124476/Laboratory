<Page x:Class="Desktop.Pages.PageLaboratory"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Desktop.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageLaboratory">

    <Grid>
        <StackPanel>
            <TextBlock Name="TextTimer" HorizontalAlignment="Center">02:00</TextBlock>
            <Button>Принять опасные расходы</Button>
            <Button>Сформировать отчеты</Button>
        </StackPanel>
    </Grid>
</Page>
