<Page x:Class="Desktop.Pages.PageLaboratoryResearcher"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Desktop.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageLaboratoryResearcher"
      Unloaded="Page_Unloaded">

    <Grid>
        <StackPanel>
            <TextBlock Name="TextTimer" HorizontalAlignment="Center">2:00</TextBlock>
            <Button>Работать с утилизатором</Button>
        </StackPanel>
    </Grid>
</Page>
